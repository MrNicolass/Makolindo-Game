function scr_gkeys(){
	
	global.ArrowUp = vk_up
    global.ArrowDown = vk_down
    global.ArrowLeft = vk_left
    global.ArrowRight = vk_right

    global.KeyUp = ord("W")
    global.KeyDown = ord("S")
    global.KeyLeft = ord("A")
    global.KeyRight = ord("D")
	
	global.KeyInteract = ord("E")
	global.KeyInteract2 = vk_enter

	global.KeyBack = vk_escape
	global.KeyBack2 = vk_backspace
}